{
  "name": "birim-web-local",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run generate-sitemap && npm run generate-robots",
    "preview": "vite preview",
    "import-media": "cd tools/media-importer && npm install && npm run import --",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 200",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "analyze": "ANALYZE=true npm run build",
    "generate-sitemap": "tsx scripts/generate-sitemap.ts",
    "generate-robots": "tsx scripts/generate-robots.ts",
    "prepare": "husky install",
    "size-limit": "size-limit",
    "size-limit:ci": "size-limit --ci",
    "email:server": "node email-server.js"
  },
  "dependencies": {
    "@fontsource/zalando-sans-semiexpanded": "^5.2.1",
    "@google/genai": "^1.28.0",
    "@sanity/client": "^6.21.3",
    "@sanity/image-url": "^1.0.2",
    "@sentry/react": "^10.26.0",
    "@tanstack/react-query": "^5.90.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/dompurify": "^3.0.5",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dompurify": "^3.3.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "groq": "^3.53.3",
    "nodemailer": "^6.9.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.23.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@size-limit/preset-app": "^11.2.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "size-limit": "^11.2.0",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.20.6",
    "typescript": "^5.2.2",
    "vite": "^7.2.4",
    "vitest": "^4.0.14",
    "web-vitals": "^4.2.4"
  }
}
